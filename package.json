    "autoprefixer": "^10.4.17",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.4.1",
    "express": "^5.1.0",
    "googleapis": "^150.0.1",
    "iconv-lite": "^0.6.3",
    "lucide-react": "^0.525.0",
