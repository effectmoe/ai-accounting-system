{
  "name": "mastra-accounting-automation",
  "version": "1.0.0",
  "description": "AI-driven accounting system automation with Mastra for Japanese tax compliance",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:supabase": "node scripts/test-supabase.js",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "generate:types": "supabase gen types typescript --local > types/supabase.ts",
    "agent:run": "tsx scripts/run-agent.ts",
    "workflow:run": "tsx scripts/run-workflow.ts",
    "mastra:dev": "tsx --watch src/index.ts",
    "mastra:start": "tsx src/index.ts",
    "mastra:api": "tsx src/api.ts",
    "mastra:build": "tsc --build",
    "mastra:workflow": "tsx scripts/run-workflow.ts",
    "mastra:agent": "tsx scripts/run-agent.ts",
    "setup": "./scripts/setup.sh",
    "setup:supabase": "./scripts/supabase-setup.sh",
    "import:freee": "tsx scripts/import-freee.ts",
    "deploy:gas-ocr": "tsx scripts/deploy-gas-ocr.ts",
    "mcp:test": "tsx scripts/test-mcp-connection.ts",
    "mastra:daily": "tsx scripts/mastra-automation.ts daily",
    "mastra:monthly": "tsx scripts/mastra-automation.ts monthly",
    "gas:auth": "tsx scripts/gas-auth.ts"
  },
