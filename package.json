    "mastra:monthly": "tsx scripts/mastra-automation.ts monthly",
    "gas:auth": "tsx scripts/gas-auth.ts",
    "gas:deploy": "tsx scripts/gas-deploy.ts",
    "gas:test": "tsx scripts/gas-test.ts",
    "gas:update": "tsx scripts/gas-update.ts",
    "gas:pull": "tsx scripts/gas-pull.ts",
    "gas:deploy-ocr": "tsx src/scripts/manual-gas-deploy.ts",
    "test:ocr-flow": "tsx src/scripts/test-ocr-flow.ts",
    "gas:show-code": "tsx src/scripts/gas-code-replacement.ts",
    "gas:update-direct": "tsx src/scripts/direct-gas-update.ts",
    "test:integration": "tsx tests/integration-test.ts",
    "test:integration:deploy": "DEPLOY_TEST=true tsx tests/integration-test.ts",
    "setup:vercel-env": "tsx scripts/setup-vercel-env.ts",
    "setup:mongodb": "tsx scripts/mongodb-setup.ts",
    "migrate:supabase-to-mongodb": "tsx scripts/migrate-supabase-to-mongodb.ts",
    "test:azure-mongodb": "tsx scripts/test-azure-mongodb.ts",
    "db:setup": "tsx scripts/setup-db.ts",
    "db:setup:with-sample": "tsx scripts/setup-db.ts --sample-data",
    "mcp:setup": "tsx scripts/setup-mcp-servers.ts",
    "mcp:debug:coordinator": "node src/debug/mcp-servers/aam-debug-coordinator.js",
    "mcp:debug:ocr": "node src/debug/mcp-servers/ocr-agent-monitor.js",
    "mcp:debug:install": "cd src/debug/mcp-servers && npm install"
