# Test environment variables
# Copy this file to .env and fill in your actual values

# DeepSeek API (Required for Mastra agents)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Supabase Configuration (Optional for DB tests)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Google Cloud (Optional for actual OCR)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/credentials.json

# GitHub (Optional for deployment tests)
GITHUB_TOKEN=your_github_token_here

# Vercel (Optional for deployment tests)
VERCEL_TOKEN=your_vercel_token_here

# Report deployment password
REPORT_PASSWORD=test_password_2024

# Test mode flags
TEST_MODE=true
SKIP_EXTERNAL_APIS=true