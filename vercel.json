{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["hnd1"],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "app/api/ocr/pdf/route.ts": {
      "maxDuration": 30
    },
    "app/api/import/freee/route.ts": {
      "maxDuration": 60
    },
    "app/api/batch-import/route.ts": {
      "maxDuration": 60
    }
  }
}