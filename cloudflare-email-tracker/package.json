{
  "name": "accounting-email-tracker",
  "version": "1.0.0",
  "description": "Email tracking service for AI Accounting System",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create accounting-email-events",
